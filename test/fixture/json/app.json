{
    "pages": [
        {
            "path": "pages/tab-bar/tab1/tab1",
            "title": "首页",
            "name": "home"
        },
        "pages/tab-bar/tab4/tab4",
    ],
    "subpackages": [
        {
            "root": "pkg-demo",
            "pages": [
                {
                    "path": "pages/index/index",
                    "title": "示例",
                    "name": "demo",
                    "meta": {
                        "auth": false
                    }
                },
                "pages/select-user/select-user",
            ]
        },
    ],
    "window": {
        "navigationBarBackgroundColor": "#ffffff",
        "navigationBarTitleText": "测试号",
        "navigationBarTextStyle": "black",
        "navigationStyle": "default",
        "backgroundColor": "#F6F6F6",
        "backgroundTextStyle": "dark",
        "onReachBottomDistance": 50
    },
    "tabBar": {
        "custom": process.env.CUSTOM_TAB_BAR,
        "color": "#000000",
        "selectedColor": "#5E48F6",
        "backgroundColor": "#ffffff",
        "borderStyle": "black",
        "list": [
            {
                "pagePath": "pages/tab-bar/tab1/tab1",
                "text": "首页",
                "iconPath": "images/wx.png",
                "selectedIconPath": "images/wx.png"
            },
            {
                "pagePath": "pages/tab-bar/tab4/tab4",
                "text": "我的",
                "iconPath": "images/wx.png",
                "selectedIconPath": "images/wx.png"
            }
        ],
        "position": "bottom"
    },
    "usingComponents": {
        "i-button": "@weapp/button",
        "i-icon": "@weapp/icon",
    },
    "networkTimeout": {
        "request": 60000,
        "connectSocket": 60000,
        "uploadFile": 60000,
        "downloadFile": 60000
    },
    "resizable": true,
    "sitemapLocation": "sitemap.json"
}
